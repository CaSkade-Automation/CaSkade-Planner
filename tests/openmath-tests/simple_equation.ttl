@prefix ont: <http://example.org/ontology#>.
@prefix m: <http://openmath.org/vocab/math#>.

ont:myApplication_equals
	a m:Application;
	m:arguments ( ont:x ont:y);
m:operator "http://www.openmath.org/cd/relation1#eq".

ont:x a m:Variable; 
	m:name "x".

ont:y a m:Variable; 
	m:name "y".
	